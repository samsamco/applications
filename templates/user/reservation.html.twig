                        <div class="col-12">
                            <div class="row mt-3">
                                <div class="col-6">
                                    <div class="d-flex align-items-center"> <input type="checkbox" name="checkbox" class="checkall accepter" disabled /> <span class="colorbrand d-inline-block ml-2">Accépter</span> </div>
